	<queryString language="SQL">
		<![CDATA[select tblpedido.idpedido, tblentidade.nome, tblpedido.idusuario, tblusuario.nome, 
tblpedido.data, tblpedido.dataentrega, tblferragem.idferragem, tblferragem.qtde, tblferragem.comprimento,
tblitemferragem.idproduto, tblproduto.descricao, tblitemferragem.diametro, tblitemferragem.qtdeferro, 
tblitemferragem.qtdepecas, tblitemferragem.qtdematerial from tblpedido
left join tblentidade on tblentidade.idcliente = tblpedido.idcliente
left join tblusuario on tblusuario.idusuario = tblpedido.idusuario
left join tblferragem on tblferragem.idpedido = tblpedido.idpedido
left join tblitemferragem on tblitemferragem.idferragem = tblferragem.idferragem
left join tblproduto on tblproduto.idproduto = tblitemferragem.idproduto
order by tblpedido.idpedido]]>
	</queryString>
