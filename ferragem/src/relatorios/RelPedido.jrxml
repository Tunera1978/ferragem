	<queryString language="SQL">
		<![CDATA[SELECT
     tblpedido.`idpedido` AS tblpedido_idpedido,
     tblpedido.`data` AS tblpedido_data,
     tblpedido.`dataentrega` AS tblpedido_dataentrega,
     tblpedido.`idcliente` AS tblpedido_idcliente,
     tblpedido.`idusuario` AS tblpedido_idusuario,
     tblferragem.`idferragem` AS tblferragem_idferragem,
     tblferragem.`qtde` AS tblferragem_qtde,
     tblferragem.`estriboaltura` AS tblferragem_estriboaltura,
     tblferragem.`estribolargura` AS tblferragem_estribolargura,
     tblferragem.`comprimento` AS tblferragem_comprimento,
     tblferragem.`estriboespaco` AS tblferragem_estriboespaco,
     tblitemferragem.`diametro` AS tblitemferragem_diametro,
     tblitemferragem.`qtdeferro` AS tblitemferragem_qtdeferro,
     tblitemferragem.`qtdepecas` AS tblitemferragem_qtdepecas,
     tblitemferragem.`qtdematerial` AS tblitemferragem_qtdematerial,
     tblusuario.`nome` AS tblusuario_nome,
     tblproduto.`descricao` AS tblproduto_descricao,
     tblentidade.`nome` AS tblentidade_nome
FROM
     `tblpedido` tblpedido INNER JOIN `tblferragem` tblferragem ON tblpedido.`idpedido` = tblferragem.`idpedido`
     INNER JOIN `tblitemferragem` tblitemferragem ON tblferragem.`idferragem` = tblitemferragem.`idferragem`
     INNER JOIN `tblproduto` tblproduto ON tblitemferragem.`idproduto` = tblproduto.`idproduto`
     INNER JOIN `tblusuario` tblusuario ON tblpedido.`idusuario` = tblusuario.`idusuario`
     INNER JOIN `tblentidade` tblentidade ON tblpedido.`idcliente` = tblentidade.`idcliente`]]>
	</queryString>
